    "version": "0.2.0",
    "configurations": [
        {
            "name": "Win Import",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["import", "-dir=D:/data/hacker news data"],
            //"trace": "log"
        },
        {
            "name": "Win Status",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["status"],
            //"trace": "log"
        },
        {
            "name": "Win Rank",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["rank", "-filter=awesome","-conf=awesome.json", "-commentLimit=50000"],
            //"args": ["rank", "-filter=docker", "-conf=docker.json"],
            //"trace": "log"
        },
        {
            "name": "Win Talk",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["talk", "-conf=awesome.json", "-verbose=true", "-init=\"I love the\"", "-randInit=1", "-randTalk=1"],
            //"trace": "log"
        }
    ]
}